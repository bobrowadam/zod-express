{
  "name": "@shaharke/zod-express",
  "version": "0.1.6",
  "description": "Express middleware for input validation and parsing using [Zod](https://zod.dev).",
  "author": {
    "name": "Adam Bobrow",
    "url": "https://github.com/bobrowadam"
  },
  "contributors": [
    {
      "name": "Shahar Kedar",
      "url": "https://github.com/shaharke"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bobrowadam/zod-express"
  },
  "bugs": {
    "url": "https://github.com/bobrowadam/zod-express/issues"
  },
  "scripts": {
    "test:unit": "vitest run --coverage",
    "test:lint": "pnpm run lint",
    "test:compile": "tsc --noEmit",
    "test": "pnpm run test:unit && pnpm run test:lint && pnpm run test:compile",
    "build": "tsup && attw --pack",
    "lint": "./node_modules/.bin/eslint ."
  },
  "type": "module",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.15.2",
    "@tsconfig/node18": "18.2.2",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.0",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitest/coverage-istanbul": "1.4.0",
    "axios": "^1.6.7",
    "eslint": "latest",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "express": "4.18.3",
    "jest": "29.7.0",
    "ts-jest": "29.1.2",
    "tsup": "8.0.2",
    "typescript": "5.4.2",
    "typescript-eslint-language-service": "5.0.5",
    "vitest": "1.4.0",
    "zod": "3.22.4"
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "peerDependencies": {
    "express": "^4.18.3",
    "zod": "^3.0.0"
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0"
  },
  "files": [
    "dist",
    "*.md"
  ],
  "keywords": [
    "nodejs",
    "api",
    "http",
    "middleware",
    "json",
    "express",
    "typescript",
    "schema",
    "server",
    "handler",
    "schema-validation",
    "zod",
    "validation"
  ]
}
